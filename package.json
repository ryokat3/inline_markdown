{
  "name": "inline_markdown",
  "version": "0.5.1",
  "description": "A customizable HTML template to self-render the inlined Markdown text",
  "main": "main.js",
  "scripts": {
    "build": "npm run build:webpack && npm run build:inline",
    "build:webpack": "webpack-cli",
    "build:inline": "inline-source --root html html/template.html inline_markdown.html",
    "view": "firefox inline_markdown.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "https://github.com/wak109",
  "license": "MIT",
  "devDependencies": {
    "inline-source": "^7.2.0",
    "inline-source-cli": "^2.0.0",
    "ts-loader": "^8.0.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@types/highlight.js": "^9.12.4",
    "@types/marked": "^1.1.0",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.1.2",
    "marked": "^1.1.1"
  }
}
