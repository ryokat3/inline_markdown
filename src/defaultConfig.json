{
    "topPage": "index.md",
    "markdownFileRegex": [ ".*\\.(md|markdown)$" ],    
    "markdownBodyClass": "markdown-body",
    "configFile": "wikipack.config.json",
    "minCheckInterval": 10,
    "maxCheckInterval": 2000,
    "packCss": true,
    "cssRules" : [
        {
            "regex": ".*",
            "cssList": [
                "https://cdnjs.cloudflare.com/ajax/libs/github-markdown-css/5.5.1/github-markdown.min.css",
                "https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.9.0/styles/github.min.css"                
            ]
        }
    ]
}